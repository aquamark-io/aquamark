{
  "manifest_version": 3,
  "name": "Aquamark Email Fetch Test",
  "description": "Test fetching Gmail user email",
  "version": "1.0",
  "permissions": [
    "identity",
    "activeTab",
    "identity.email"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "aquamark_icon.png",
      "48": "aquamark_icon.png",
      "128": "aquamark_icon.png"
    }
  },
  "icons": {
    "128": "aquamark_icon.png"
  }
}
